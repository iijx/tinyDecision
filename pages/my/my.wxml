<view class="container">
    <view class="userInfo">
        <open-data class="avatar" type="userAvatarUrl"></open-data>
        <open-data class="nickName" type="userNickName"></open-data>
    </view>

    <view class="operators">
        <!-- <view class="operator" bindtap="wait">模板</view> -->
        <view class="operator" bindtap="wait">
            <view>模板</view>
            <view class="arrow-right"></view>
        </view>
        <view class="operator" bindtap="wait">高级设置</view>
    </view>

    <view class="operators">
        <!--
        <view class="operator">常见问题</view>
        -->
        <view class="operator">
            <button class="contact-button" open-type="contact">建议与联系</button>
            <view class="arrow-right"></view>
        </view>
        <view class="operator" bindtap="pageToAbout">
            <view>关于</view>
            <view class="arrow-right"></view>
        </view>
    </view>

</view>