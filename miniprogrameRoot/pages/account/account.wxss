.collapse-item {
    border-top: none;
}
.container {
    padding: 34rpx;
}


.user-info-wrap {
    width: 686rpx;
    height: 260rpx;
    border-radius: 2rpx;
    background-image: url('https://iijx-cdn.oss-cn-beijing.aliyuncs.com/minip/tinydecision/coin-bg.png');
    background-position: bottom center;
    background-repeat: no-repeat;
    background-size: 100% auto;
    box-sizing: border-box;
    text-align: center;
    padding: 54rpx 32rpx 48rpx;
    display: flex;
    color: #fff;
}
.user-info-wrap .left,
.user-info-wrap .right {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.user-info-wrap .left {
    padding-right: 40rpx;
}
.user-info-wrap .right {
    padding-left: 40rpx;
}
.user-info-wrap .avatar {
    width: 96rpx;
    height: 96rpx;
    margin: 0 auto;
    border-radius: 50%;
    overflow: hidden;
}
.user-info-wrap .avatar  .img {
    width: 100%;
    height: 100%;
    display: block;
    border-radius: 50%;
}
.user-info-wrap .nickname {
    font-size: 34rpx;
    font-weight: 600;
}
.user-info-wrap .num {
    font-size:76rpx;
    font-weight: 600;
    height: 106rpx;
}
.user-info-wrap .label {
    font-size: 26rpx;
    height: 46rpx;
    line-height: 46rpx;
}


.note-wrap {
    background: #fff;
    border-radius: 10rpx;
    margin-top: 32rpx;
    font-size: 34rpx;
    padding-left: 34rpx;
}